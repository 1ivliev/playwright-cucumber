{
  "name": "playwright-cucumber",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "test": "cucumber-js --parallel 1 --exit",
    "test:qase": "cucumber-js -f cucumberjs-qase-reporter --format-options='{\"qaseConfig\": \"./.qaserc\"}' features -r features/support/steps -r features/support/word",
    "dev": "npm run lint && tsc",
    "dev:watch": "nodemon --exec 'npm run dev || exit 1'",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "build": "npm run clean && npm run dev",
    "clean": "rm -rf dist || true",
    "example": "npm run build && cd examples && npm run test-zombie"
  },
  "keywords": [],
  "author": "Kirill Ivliev",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.20.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "chalk": "^4.1.0",
    "moment": "^2.29.1",
    "qaseio": "^2.0.0"
  },
  "devDependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "cucumberjs-qase-reporter": "^0.2.8-alpha.0",
    "playwright": "^1.21.1",
    "@cucumber/cucumber": "^7.0.0",
    "@cucumber/messages": "^14.0.1",
    "@hutson/npm-deploy-git-tag": "^6.0.0",
    "@types/jest": "^23.3.7",
    "@types/long": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "ansi-regex": "^6.0.1",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.17.0",
    "eslint-plugin-jsdoc": "^26.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "handlebars": "^4.7.7",
    "jest": "^26.6.3",
    "json-schema": "^0.4.0",
    "long": "^4.0.0",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.3"
  }
}
